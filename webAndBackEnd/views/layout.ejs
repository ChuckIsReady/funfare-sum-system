  <body>
    <div class="container">
     <div class="row">
          <ul  class="header-nav col-xl-12">
            <li class="Funfares active"><a href="/">Funfares</a></li>
            <li class="Search  "><a href="/funfare/Search">Search  </a></li>

          <%if(username=='admin'){%>
            <li class="Create  "><a href="/funfare/Create ">Create  </a></li>
            <li class="Admin   "><a href="/funfare/Admin ">Admin   </a></li>
            <li class="float-right "><a href="/user/logout ">Login Out </a></li>
            <li class="float-right">Hi, Admin!</li>
            
            <%} else if (username!=undefined) {%>
              <li class="Admin   "><a href="/order/orderList ">My Package  </a></li>
              <li class="float-right "><a href="/user/logout ">Login Out </a></li>
              <li class="float-right">Hi, <%=username%>!</li>
             <%} else{%> 
            <li class="Admin  float-right "><a href="/user/login ">Login   </a></li>
            <%}%>
          </ul>
    </div>
    <div class="row">
      <div class="logo col-xl-12">
        <div class="row">
          <div class="logo-title">Funfares</div><img  class="logo-img" src="/img/airplane.png" alt="">
        </div>
        
        <div class="logo-text">Limited time,Limited seats. Full of FUN.</div>
      </div>
    </div>
      <%- body %>
    </div>




